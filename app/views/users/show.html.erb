<h1> User Home </h1>

<br>
    <% flash.each do |type, msg| %>
        <h2>
        <%= msg %>
        </h2>
    <% end %>

<br>
<h3>Choose an Adventure!</h3>
    <% Adventure.all.each do |adv| %>
        <%= link_to adv.title, adventure_path(adv) %>
        <br>
        <b>Boss: <%= adv.boss%></b>
        <p>
        <%= adv.flavor%>
        </p>
    <% end %>
