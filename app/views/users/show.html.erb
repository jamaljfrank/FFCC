<h1> User Home </h1>
    <% if admin%>
        <%= link_to "Create Adventure", new_adventure_path %>
    <%end%>
<br>
    <% flash.each do |type, msg| %>
        <div>
        <%= msg %>
        </div>
    <% end %>

<br>
<h2>Adventures</h2>
    <% Adventure.all.each do |adv| %>
        <%= link_to adv.title, adventure_path(adv) %>
        <br>
        <b>Boss: <%= adv.boss%></b>
        <p>
        <%= adv.flavor%>
        </p>
    <% end %>
